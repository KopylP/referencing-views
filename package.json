{
	"name": "referencing-views",
	"displayName": "referencing-views",
	"description": "",
	"publisher": "referencing-views",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0",
		"azdata": "*"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:referencing-views.show"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": {
			"command": "referencing-views.show",
			"title": "Show referencing views"
		},
		"menus": {
			"objectExplorer/item/context": [
				{
					"command": "referencing-views.show",
					"when": "connectionProvider == MSSQL && nodeType == Table || nodeType == View",
					"group": "NONE"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"proposedapi": "node installTypings.js"
	},
	"devDependencies": {
		"@types/azdata": "*",
		"@types/chai": "^4.3.4",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.39.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"azdata-test": "^1.x",
		"chai": "^4.3.7",
		"eslint": "^6.8.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.0",
		"typescript": "^4.3.5"
	}
}
